    "n = int(input())\n",
    "s = []\n",
    "\n",
    "for i in range(n):\n",
    "    x, y = map(int, input().split())\n",
    "    s.append((x, y))\n",
    "\n",
    "s.sort(key = lambda i:(i[1], i[0]))\n",
    "\n",
    "\n",
    "for i in s:\n",
    "    print(i[0], i[1])"
