   "source": [
    "def hanoi(n, bar1, bar3):\n",
    "    if n == 1:\n",
    "        print(bar1, bar3)\n",
    "        return\n",
    "    \n",
    "    hanoi(n-1, bar1, 6-bar1-bar3)\n",
    "    print(bar1, bar3)\n",
    "    hanoi(n-1, 6-bar1-bar3, bar3)\n",
    "    \n",
    "    \n",
    "n = int(input())\n",
    "print(2**n - 1)\n",
    "hanoi(n, 1, 3)"
   ]
  }
