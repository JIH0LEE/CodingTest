    "# 선택정렬\n",
    "n = int(input())\n",
    "s = []\n",
    "\n",
    "for i in range(n):\n",
    "    s.append(int(input()))\n",
    "\n",
    "for i in range(len(s) - 1):\n",
    "    tmp = i\n",
    "    for j in range(i + 1, len(s)):\n",
    "        if(s[tmp] >= s[j]):\n",
    "            tmp = j\n",
    "    s[i], s[tmp] = s[tmp], s[i]\n",
    "\n",
    "for i in range(n):\n",
    "    print(s[i])"
   ]
  }
